generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id        Int      @id @default(autoincrement())
  title     String
  desc      String
  image     String
  gallery   String[] @default([])
  link      String?
  github    String?
  tech      String[]
  createdAt DateTime @default(now())
}
